{
  "name": "rate-limiter",
  "version": "1.0.0",
  "description": "Rate Limiter Implementation from scratch!",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start-token-bucket": "nodemon --watch 'src/**/*' -e ts --exec ts-node src/TokenBucket/app.ts",
    "start-fixed-window-counter": "nodemon --watch 'src/**/*' -e ts --exec ts-node src/FixedWindowCounter/app.ts",
    "start-sliding-window-log": "nodemon --watch 'src/**/*' -e ts --exec ts-node src/SlidingWindowLog/app.ts",
    "start-sliding-window-counter": "nodemon --watch 'src/**/*' -e ts --exec ts-node src/SlidingWindowCounter/app.ts",
    "test": "vitest"
  },
  "keywords": [
    "rate-limiter",
    "Nevil Parmar"
  ],
  "author": {
    "name": "Nevil Parmar",
    "url": "https://nevilparmar.netlify.app/"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "express": "^4.18.3",
    "supertest": "^6.3.4"
  }
}
